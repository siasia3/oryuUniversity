<update id="internshipAPCUpdate" parameterType="String">
UPDATE INTERNSHIP_APC
SET
IA_PUR = #{iaPur, jdbcType=CLOB}
,IA_COU= #{iaCou, jdbcType=CLOB}
WHERE IA_CD = #{iaCd}
</update>

<update id="internshipAPCPass" parameterType="String">
UPDATE INTERNSHIP_APC
SET
IA_STAT = #{pass}
WHERE IA_CD=#{iaCd}
</update>


</mapper>