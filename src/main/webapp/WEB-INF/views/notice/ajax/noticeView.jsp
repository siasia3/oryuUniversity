<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ taglib uri="http://www.springframework.org/tags" prefix="spring"%>
<%@ taglib uri="http://www.springframework.org/tags/form" prefix="form"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn"%>
<div class="card">
	<div class="card-body">
		<form class="form-sample">
			<div class="row">
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">알림코드</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.notiCd }" />
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">알림종류</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.notiType}" />
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">발신자(코드)</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.sender}(${notification.notiSender})" />
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">작성일자</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.notiDate}" />
						</div>
					</div>
				</div>
			</div>
						<div class="row">
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">제목</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.notiTitle }" />
						</div>
					</div>
				</div>
				<div class="col-md-6">
					<div class="form-group row">
						<label class="col-sm-3 col-form-label">내용</label>
						<div class="col-sm-9">
							<input type="text" class="form-control" readonly
								value="${notification.notiCont}" />
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
</div>

